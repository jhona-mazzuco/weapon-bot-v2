{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "rg/metacritic/(.*)": "<rootDir>/../libs/metacritic/src/$1",
    "rg/metacritic": "<rootDir>/../libs/metacritic/src"
  }
}